<Window x:Class="MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:metro="http://schemas.codeplex.com/elysium"
    Title="CastarJupiter 『JupiterManager』 [ Ver 2015 ]" Height="1024" Width="1600" HorizontalAlignment="Center" Background="#FFFFFFFF" Foreground="Black" x:Name="MainWindow" WindowStartupLocation="CenterScreen" >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="0"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0"/>
            <RowDefinition Height="57.5"/>
            <RowDefinition Height="17.5"/>
            <RowDefinition Height="10"/>
            <RowDefinition Height="25"/>
            <RowDefinition Height="10"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="0"/>
        </Grid.RowDefinitions>
        <Button Name="ButtonNew" Grid.Column="1" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="新規工事作成" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/01_New.png"  x:Name="Image_New"/>
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="1" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="新規工事" Grid.Column="1" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonOpen" Grid.Column="2" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="既存工事を開く" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/02_Open.png" x:Name="Image_Open"/>
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="2" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="工事を開く" Grid.Column="2" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonKojiEdit" Grid.Column="3" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="工事属性を変更" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/03_KojiEdit.png" x:Name="Image_KojiEdit" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="3" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="工事属性変更" Grid.Column="3" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonJconsole" Grid.Column="5" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="Jconsoleを起動" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/04_Jconsole.png" x:Name="Image_Jconsole" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="5" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="Jconsole" Grid.Column="5" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonLog" Grid.Column="6" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="ログを開く" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/05_Log.png" x:Name="Image_Log" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="6" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="ログを開く" Grid.Column="6" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonStandard" Grid.Column="7" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="STANDARD.DATを開く" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/06_Standard.png" x:Name="Image_Standard" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="7" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="標準値設定" Grid.Column="7" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonWeight" Grid.Column="8" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="重量計算" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/07_Weight.png" x:Name="Image_Weight" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="8" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="重量計算" Grid.Column="8" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonPmf" Grid.Column="10" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="PMFを描画" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/08_Pmf.png" x:Name="Image_Pmf" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="10" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="PMF描画" Grid.Column="10" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonCl" Grid.Column="11" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="CLファイルを描画" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/09_Cl.png" x:Name="Image_Cl" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="11" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="CL描画" Grid.Column="11" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonDxf" Grid.Column="12" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="DXFファイルを表示" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/10_Dxf.png" x:Name="Image_Dxf" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="12" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="DXF描画" Grid.Column="12" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonIJCAD" Grid.Column="14" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="IJCADを起動" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/11_IJCAD.png" x:Name="Image_IJCAD" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="14" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="IJCAD" Grid.Column="14" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonEXCEL" Grid.Column="15" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="Excelを起動" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/12_EXCEL.png" x:Name="Image_EXCEL" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="15" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="EXCEL" Grid.Column="15" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonACCESS" Grid.Column="16" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="Accessを起動" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/13_ACCESS.png" x:Name="Image_ACCESS" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="16" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="ACCESS" Grid.Column="16" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonSetting" Grid.Column="21" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="設定" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/16_SETTING.png" x:Name="Image_SETTING" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="21" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="SETTING" Grid.Column="21" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Button x:Name="ButtonHelp" Grid.Column="22" HorizontalAlignment="Center" Height="56" Grid.Row="1" VerticalAlignment="Center" Width="56" ToolTip="HELP" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
            <Image Source="MenuIcon/17_HELP.png" x:Name="Image_HELP" />
        </Button>
        <Border BorderBrush="#FFDCDCDC" BorderThickness="1" Grid.Column="22" HorizontalAlignment="Left" Height="75" Grid.Row="1" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
        <Label Content="HELP" Grid.Column="22" HorizontalAlignment="Center" Height="20" Grid.Row="2" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>

        <Label Content="工 号" Grid.Column="1" HorizontalAlignment="Left" Height="25" Grid.Row="4" VerticalAlignment="Top" Width="75" Foreground="Black" Margin="10,0,0,0"/>
        <TextBox x:Name="TextBoxKogo" Grid.ColumnSpan="2" Grid.Column="2" HorizontalAlignment="Left" Height="25" Grid.Row="4" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" BorderBrush="Black" IsEnabled="False"/>
        <Label Content="工事名" Grid.Column="5" HorizontalAlignment="Left" Height="25" Grid.Row="4" VerticalAlignment="Top" Width="75" Foreground="Black"/>
        <TextBox x:Name="TextBoxKojiName" Grid.ColumnSpan="5" Grid.Column="6" HorizontalAlignment="Left" Height="25" Grid.Row="4" Grid.RowSpan="2" TextWrapping="Wrap" VerticalAlignment="Top" Width="320" BorderBrush="Black" IsEnabled="False"/>
        <Label Content="工事フォルダ" Grid.Column="12" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="25" Grid.Row="4" VerticalAlignment="Top" Foreground="Black"/>
        <TextBox x:Name="TextBoxKojiFolder" Grid.ColumnSpan="4" Grid.Column="14" HorizontalAlignment="Stretch" Height="25" Grid.Row="4" TextWrapping="Wrap" VerticalAlignment="Top" Margin="0,0,0,0" BorderBrush="Black" IsEnabled="False"/>
        <Label Content="構造形式" Grid.Column="19" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="25" Grid.Row="4" VerticalAlignment="Top" Foreground="Black"/>
        <TextBox x:Name="TextBoxKozo" Grid.ColumnSpan="2" Grid.Column="21" HorizontalAlignment="Stretch" Height="25" Grid.Row="4" TextWrapping="Wrap" VerticalAlignment="Top" Margin="0,0,10,0" BorderBrush="Black" IsEnabled="False"/>
        <TabControl x:Name="TabCtl" Grid.ColumnSpan="22" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="6" VerticalAlignment="Stretch" Background="Gainsboro" BorderBrush="Gainsboro" >
            <TabItem Header="  骨組関連  " Name="TabSkl" Style="{DynamicResource TabItemStyle}">
                <Grid Margin="0,-1,0,1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="80"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="250"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="600"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="75"/>
                        <ColumnDefinition Width="75"/>
                        <ColumnDefinition Width="10"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="10"/>
                        <RowDefinition Height="265"/>
                        <RowDefinition Height="10"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="5"/>
                        <RowDefinition Height="57.5"/>
                        <RowDefinition Height="17.5"/>
                        <RowDefinition Height="75"/>
                        <RowDefinition Height="5"/>
                    </Grid.RowDefinitions>
                    <Button x:Name="ButtonSklSave" Grid.Column="8" HorizontalAlignment="Center" Height="56" Grid.Row="5" VerticalAlignment="Center" Width="56" ToolTip="保存" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
                        <Image Source="MenuIcon/18_SAVE.png" x:Name="Image_SklSave"/>
                    </Button>
                    <Border BorderBrush="#FF838282" BorderThickness="1" Grid.Column="8" HorizontalAlignment="Left" Height="75" Grid.Row="5" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
                    <Label Content="保存" Grid.Column="8" HorizontalAlignment="Center" Height="20" Grid.Row="6" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>
                    <Button x:Name="ButtonSklEdit" Grid.Column="9" HorizontalAlignment="Center" Height="56" Grid.Row="5" VerticalAlignment="Center" Width="56" ToolTip="エディタで編集" Background="White" Panel.ZIndex="2" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand">
                        <Image Source="MenuIcon/19_TxtEdit.png" x:Name="Image_SklEdit"/>
                    </Button>
                    <Border BorderBrush="#FF838282" BorderThickness="1" Grid.Column="9" HorizontalAlignment="Left" Height="75" Grid.Row="5" VerticalAlignment="Top" Width="75" Background="#FFFFFFFF" Grid.RowSpan="2"/>
                    <Label Content="エディタ編集" Grid.Column="9" HorizontalAlignment="Center" Height="20" Grid.Row="6" VerticalAlignment="Top" Width="75" FontSize="10" HorizontalContentAlignment="Center" Padding="5,2,5,2"/>
                    <GroupBox x:Name="GroupBoxSkmake"   Grid.ColumnSpan="4" Grid.Column="1" Header="骨組作成" HorizontalAlignment="Stretch" Grid.Row="1" Grid.RowSpan="1" VerticalAlignment="Stretch">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="90"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="5"/>
                            </Grid.ColumnDefinitions>
                            <Button x:Name="ButtonSklPknd" Content="横断線種類&#10;確認・変更" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyleText}" Cursor="Hand" Background="#FF5A5A5A" Foreground="White" FontSize="11"/>
                            <Button x:Name="ButtonSklData" Content="骨組データ&#10;確認・変更" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="5" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyleText}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <Button x:Name="ButtonSkmake" Content="骨組作成" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="9" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <Image x:Name="ImageSklLow1" Source="Picture/Y01_LOW.png" Grid.Column="1" Grid.Row="3"/>
                            <Image x:Name="ImageSklLow2" Source="Picture/Y01_LOW.png" Grid.Column="1" Grid.Row="7"/>
                            <Label x:Name="LabelSklPknd" Content="線形ファイルを読み込む際に横断線種類&#10;(支点・格点)などを定義するファイルを&#10;確認・変更します。" Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Stretch" VerticalContentAlignment="Center" Padding="5,0" FontWeight="Bold" FontSize="11"/>
                            <Label x:Name="LabelSklData" Content="骨組の入力データを確認・変更します。" Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="5" VerticalAlignment="Stretch" VerticalContentAlignment="Center" Padding="5,0" FontWeight="Bold" FontSize="11"/>
                            <Label x:Name="LabelSkmake" Content="完成形と製作形の骨組ファイル(*.SKL)&#10;を作成します。" Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="9" VerticalAlignment="Stretch" VerticalContentAlignment="Center" Padding="5,0" FontWeight="Bold" FontSize="11"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox x:Name="GroupBoxSkReport" Grid.ColumnSpan="4" Grid.Column="1" Header="骨組チェック・原寸検査資料関連" HorizontalAlignment="Stretch" Grid.RowSpan="4" Grid.Row="3" VerticalAlignment="Top" Height="415" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="90"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="5"/>
                            </Grid.ColumnDefinitions>
                            <Button x:Name="ButtonSklReport1" Content="Button1" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <Button x:Name="ButtonSklReport2" Content="Button2" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="3" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <Button x:Name="ButtonSklReport3" Content="Button3" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="5" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <Button x:Name="ButtonSklReport4" Content="Button4" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="7" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <Button x:Name="ButtonSklReport5" Content="Button5" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="9" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <Button x:Name="ButtonSklReport6" Content="Button6" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="11" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <Button x:Name="ButtonSklReport7" Content="Button7" Grid.Column="1" HorizontalAlignment="Stretch" Grid.Row="13" VerticalAlignment="Stretch" Style="{DynamicResource FlatButtonStyle}" Cursor="Hand" Background="Black" Foreground="White" FontSize="11"/>
                            <StackPanel Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Center" >
                                <TextBlock x:Name="LabelSklReport1" Text="Label1" Padding="5,0" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11"/>
                            </StackPanel>
                            <StackPanel Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="3" VerticalAlignment="Center" >
                                <TextBlock x:Name="LabelSklReport2" Text="Label2" Padding="5,0" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11"/>
                            </StackPanel>
                            <StackPanel Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="5" VerticalAlignment="Center" >
                                <TextBlock x:Name="LabelSklReport3" Text="Label3" Padding="5,0" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11"/>
                            </StackPanel>
                            <StackPanel Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="7" VerticalAlignment="Center" >
                                <TextBlock x:Name="LabelSklReport4" Text="Label4" Padding="5,0" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11"/>
                            </StackPanel>
                            <StackPanel Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="9" VerticalAlignment="Center" >
                                <TextBlock x:Name="LabelSklReport5" Text="Label5" Padding="5,0" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11"/>
                            </StackPanel>
                            <StackPanel Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="11" VerticalAlignment="Center" >
                                <TextBlock x:Name="LabelSklReport6" Text="Label6" Padding="5,0" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11"/>
                            </StackPanel>
                            <StackPanel Grid.Column="3" HorizontalAlignment="Stretch" Grid.Row="13" VerticalAlignment="Center" >
                                <TextBlock x:Name="LabelSklReport7" Text="Label7" Padding="5,0" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" FontSize="11"/>
                            </StackPanel>
                        </Grid>
                    </GroupBox>
                    <RichTextBox x:Name="SklRichTextBox" Grid.ColumnSpan="4" Grid.Column="6" HorizontalAlignment="Stretch" Margin="0,30,0,0" Grid.Row="1" Grid.RowSpan="3" VerticalAlignment="Stretch" FontFamily="MS Gothic" Block.LineHeight="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AcceptsTab="True">
                        <FlowDocument PageWidth="3000">
                            <Paragraph>
                                <Run Text=""/>
                            </Paragraph>
                        </FlowDocument>
                    </RichTextBox>
                    <Label Content="ファイル名=" Grid.Column="6" Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,0,0,0" Width="80" />
                    <Label x:Name="LabelSklFileName" Content="" Grid.Column="6" Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="80,0,0,0" Width="500" />
                    <GroupBox Grid.Column="6" Header="ボタンの説明" HorizontalAlignment="Stretch" Grid.Row="5" Grid.RowSpan="3" VerticalAlignment="Stretch">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="50"/>
                                <RowDefinition Height="5"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="5"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
