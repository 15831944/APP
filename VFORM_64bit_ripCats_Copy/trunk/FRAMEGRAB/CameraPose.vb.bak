Public Class CameraPose
    Public Cid As Integer 'ID（撮影位置）
    Public CFileName As String 'イメージファイル名

    Public CX As Integer 'カメラ視点中心X座標値
    Public CY As Integer 'カメラ視点中心Y座標値
    Public CZ As Integer 'カメラ視点中心Z座標値
    Public CA As Integer 'X軸回り回転角度（度）
    Public CB As Integer 'Y軸回り回転角度（度）
    Public CG As Integer 'Z軸回り回転角度（度）

    Public CflgNotUse As Integer '
    Public CflgSystemNotUse As Integer '
    Public CflgFirst As Integer '
    Public CflgSecond As Integer '


    Public CCamParamID As Integer 'カメラ内部パラメータID
    Public Sub New()
        Cid = -1
        CFileName = ""
        CX = 0
        CY = 0
        CZ = 0
        CA = 0
        CB = 0
        CG = 0
        CflgNotUse = 0
        CflgSystemNotUse = 0
        CflgFirst = 0
        CflgSecond = 0
        CCamParamID = -1
    End Sub
End Class
